<%= form_for AttendedEvent.new do |f| %>
  
  <div class="field">
    <%= f.label :user_id %><br>
    <%= f.number_field :user_id, value: '1'  %>
  </div>
  <div class="field">
    <%= f.label :event_id %><br>
    <%= f.number_field :event_id, value: '1'  %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
